elo'):\n","      random.seed(42)\n","      random.shuffle(dados_treino)\n","      losses = {'ner': 0.0}\n","\n","      for texto, anotacoes in dados_treino:\n","        exemplo = Example.from_dict(model.make_doc(texto), anotacoes)\n","        model.update([exemplo], drop=0.2, sgd = optimizer, losses = losses)\n","\n","      print(f'\\nÉpoca: {epoca+1} - Perda Média de Treino: {losses[\"ner\"]/len(dados_treino)}')\n","\n","      val_losses = {'ner': 0.0}\n","      exemplos =[]\n","\n","      for texto, anotacoes in dados_teste:\n","        exemplo = Example.from_dict(model.make_doc(texto), anotacoes)\n","        exemplos.append(exemplo)\n","\n","      for exemplo in exemplos:\n","        model.update([exemplo], drop=0.0, sgd = None, losses = val_losses)\n","\n","      print(f'\\nÉpoca: {epoca+1} - Perda Média de Validação: {val_losses[\"ner\"]/len(dados_treino)}')\n","\n","    return model"],"metadata":{"id":"-BPYPRLaSkI5","executionInfo":{"status":"ok","timestamp":1771254749687,"user_tz":180,"elapsed":15,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}}},"execution_count":12,"outputs":[]},{"cell_type":"code","source":["model_ner = treinar_modelo(dados_treino, dados_teste, 30)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LBUaSCDxcHKp","executionInfo":{"status":"ok","timestamp":1771255794324,"user_tz":180,"elapsed":1044626,"user":{"displayName":"Victor Hugo do Nascimento","userId":"04558136456232909731"}},"outputId":"67149f5e-1296-4dd8-8710-3ffd3f339356"},"execution_count":13,"outputs":[{"output_type":"stream","name":"stderr","text":["\rTreinando o Modelo:   0%|          | 0/30 [00:00<?, ?it/s]"]},{"output_type":"stream","name":"stdout","text":["\n","Época: 1 - Perda Média de Treino: 1765.2396240234375\n"]},{"output_type":"stream","name":"stderr","text":["\rTreinando o Modelo:   3%|▎         | 1/30 [00:36<17:49, 36.88s/it]"]},{"output_type":"stream","name":"stdout","text":["\n","Época: 1 - Perda Média de Validaçã